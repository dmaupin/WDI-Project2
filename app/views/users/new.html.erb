<div class="bold center-block">
<h1>SIGN UP</h1>

<%= form_for @user do |f| %>

	First Name: <%= f.text_field :first_name %>
	Last Name: <%= f.text_field :last_name %>
	Email: <%= f.text_field :email %>
	Password <%= f.password_field :password %>
	Password Confirm <%= f.password_field :password_confirmation %>
	<%= f.submit "Signup" %>

<% end %>
</div>